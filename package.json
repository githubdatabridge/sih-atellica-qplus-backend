{
    "name": "sih-atellica-qplus-backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "prestart": "npm run tsc",
        "test": "jest",
        "docker-dev": "npm install && npm run dev",
        "dev": "tsc-watch --onSuccess \"node build/index.js\"",
        "watch": "tsc-watch --onSuccess \"node build/index.js\"",
        "compile-run": "npm run tsc && node build/index.js",
        "build": "npm run tsc",
        "start": "node build/index.js",
        "tsc": "tsc",
        "win-svc-install": "npm run tsc && node ./build/service-install.js",
        "win-svc-uninstall": "node ./build/service-uninstall.js",
        "migration:create": "knex --knexfile ./src/database/knexfile.ts migrate:make",
        "migration:up": "knex --knexfile ./src/database/knexfile.ts migrate:up",
        "migration:down": "knex --knexfile ./src/database/knexfile.ts migrate:down",
        "migration:latest": "knex --knexfile ./src/database/knexfile.ts migrate:latest",
        "migration:rollback": "knex --knexfile ./src/database/knexfile.ts migrate:rollback",
        "prepare": "husky",
        "lint": "eslint src/**/*.ts"
    },
